file serviceControl.db {
    pattern {   DEVICE,   SERVICE }
            { "$(DEV)",   "BSSS" }
}

file bsssRateLimit.db {
    pattern {   DEVICE,   SERVICE,      GLOBAL }
            { "$(DEV)" ,  "BSSS",  "TPG:SYS0:1"}
}

file serviceStatus.db {
    pattern {   DEVICE,  SERVICE }
            { "$(DEV)",  "BSSS0" }
            { "$(DEV)",  "BSSS1" }
}




